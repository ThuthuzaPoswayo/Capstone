etServices() {\r\n        this.$store.dispatch('getServices');\r\n      },\r\n      deleteUser(userID) {\r\n        this.$store.dispatch(\"deleteUser\", { id: userID });\r\n      },\r\n      deleteService(serviceID) {\r\n        this.$store.dispatch(\"deleteService\", { id: serviceID });\r\n      },\r\n    },\r\n    computed: {\r\n    \r\n      users() {\r\n        return this.$store.state.user;\r\n      },\r\n      services() {\r\n        return this.$store.state.services;\r\n      },\r\n    },\r\n    mounted() {\r\n \r\n      this.getUsers();\r\n      this.getServices();\r\n    }\r\n  };\r\n</script>\r\n\r\n  "],"mappings":"AA6EE,eAAe;EACbA,OAAO,EAAE;IAEPC,QAAQA,CAAA,EAAG;MACT,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,UAAU,CAAC;IAClC,CAAC;IACDC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACF,MAAM,CAACC,QAAQ,CAAC,aAAa,CAAC;IACrC,CAAC;IACDE,UAAUA,CAACC,MAAM,EAAE;MACjB,IAAI,CAACJ,MAAM,CAACC,QAAQ,CAAC,YAAY,EAAE;QAAEI,EAAE,EAAED;MAAO,CAAC,CAAC;IACpD,CAAC;IACDE,aAAaA,CAACC,SAAS,EAAE;MACvB,IAAI,CAACP,MAAM,CAACC,QAAQ,CAAC,eAAe,EAAE;QAAEI,EAAE,EAAEE;MAAU,CAAC,CAAC;IAC1D;EACF,CAAC;EACDC,QAAQ,EAAE;IAERC,KAAKA,CAAA,EAAG;MACN,OAAO,IAAI,CAACT,MAAM,CAACU,KAAK,CAACC,IAAI;IAC/B,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACZ,MAAM,CAACU,KAAK,CAACE,QAAQ;IACnC;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IAER,IAAI,CAACd,QAAQ,CAAC,CAAC;IACf,IAAI,CAACG,WAAW,CAAC,CAAC;EACpB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}